// Test some casting

y = 1.0;
println(typeof(y) + " = " + y);
y = 1.00;
println(typeof(y) + " = " + y);
y = 0.0000001;
println(typeof(y) + " = " + y);

println("---"); 

y = cast(2, 'double');
println("double "+typeof(y) + " = " + y);

y = cast(2, 'float');
println("float "+typeof(y) + " = " + y);

y = cast(2, 'number');
println(typeof(y) + " = " + y);

y = cast(2, 'integer');
println(typeof(y) + " = " + y);

y = cast(2, 'string');
println(typeof(y) + " = " + y);

